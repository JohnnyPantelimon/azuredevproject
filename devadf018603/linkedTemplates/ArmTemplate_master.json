{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "devadf018603"
		},
		"ln_azure_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ln_azure_sql'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=devsqlserver8603.database.windows.net;Initial Catalog=dev-db"
		},
		"ln_dev_akv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dev-akv-8603.vault.azure.net/"
		},
		"ln_azure_sql_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "a9edf515-d67b-4820-ae06-a0a55675fc31"
		},
		"ln_azure_sql_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "9eafc177-552f-407a-a74f-be8d3198a3a5"
		},
		"ln_adls_devaccountjpm_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devaccountjpm.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "devadf018603_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ln_azure_sql_connectionString": {
						"value": "[parameters('ln_azure_sql_connectionString')]"
					},
					"ln_dev_akv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ln_dev_akv_properties_typeProperties_baseUrl')]"
					},
					"ln_adls_devaccountjpm_properties_typeProperties_url": {
						"value": "[parameters('ln_adls_devaccountjpm_properties_typeProperties_url')]"
					},
					"ln_azure_sql_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ln_azure_sql_properties_typeProperties_servicePrincipalId')]"
					},
					"ln_azure_sql_properties_typeProperties_tenant": {
						"value": "[parameters('ln_azure_sql_properties_typeProperties_tenant')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}